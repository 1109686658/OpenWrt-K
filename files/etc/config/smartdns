
config smartdns
	option prefetch_domain '1'
	option port '6053'
	option seconddns_port '5335'
	option seconddns_no_rule_addr '0'
	option seconddns_no_rule_nameserver '0'
	option seconddns_no_rule_ipset '0'
	option seconddns_no_rule_soa '0'
	option tcp_server '1'
	option rr_ttl '600'
	option seconddns_enabled '1'
	option server_name 'smartdns-China'
	option seconddns_tcp_server '1'
	option seconddns_server_group 'smartdns-Overseas'
	option rr_ttl_min '5'
	option seconddns_no_cache '1'
	option seconddns_no_speed_check '0'
	option cache_size '190150'
	option serve_expired '0'
	option auto_set_dnsmasq '0'
	option ipv6_server '1'
	option dualstack_ip_selection '1'
	option force_aaaa_soa '0'
	option seconddns_no_dualstack_selection '1'
	option coredump '1'
	option speed_check_mode 'tcp:443,tcp:80,ping'
	option resolve_local_hostnames '1'
	option force_https_soa '1'
	option seconddns_force_aaaa_soa '0'
	option enable_auto_update '0'
	option enabled '1'
	option old_port '6053'
	option old_enabled '1'
	option old_auto_set_dnsmasq '0'

config server
	option enabled '1'
	option type 'udp'
	option name '114'
	option ip '114.114.114.114'
	option server_group 'smartdns-China'
	option blacklist_ip '0'
	option port '53'

